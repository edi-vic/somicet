---
import conacytLogo from "@assets/cells/conacyt.png"
import cinvestavLogo from "@assets/cells/cinvestav.png"
import facmedLogo from "@assets/cells/facmed.png"
import imssLogo from "@assets/cells/imss.png"
import unamLogo from "@assets/cells/unam.png"

const logosArr =[conacytLogo, cinvestavLogo, facmedLogo, imssLogo, unamLogo]
---
<section class="footer">
  <div class="footer__logos">
    {logosArr.map((item) => 
      <img src={item} alt="">
    )}
  </div>

</section>

<style lang="scss">
  @import "@assets/library";
  .footer{
    width: 100%;
    height: 230px;
    background: $background-color;
    display: flex;
    &__logos{
      width: 100%;
      max-width: 768px;
      
      
    }

  }

</style>